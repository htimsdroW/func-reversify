{
  "compilerOptions": {
    "target": "ES2022",
    "module": "UMD",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "rootDir": "..",
    "outDir": "../dist",
    "declaration": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "composite": true
  },
  "buildOptions": {
    "incremental": true
  }
}
